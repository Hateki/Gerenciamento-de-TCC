<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="br.edu.unipampa.model.Datas" table="datas" catalog="`GERENCIAMENTO DE TCC`">
        <id name="idDatas" type="java.lang.Integer">
            <column name="idDatas" />
            <generator class="identity" />
        </id>
        <property name="dataInicioTema" type="string">
            <column name="dataInicioTema" length="45" />        
        </property>
        <property name="dataFimTema" type="string">
            <column name="dataFimTema" length="45" />        
        </property>
    
        <property name="dataInicioTcc" type="string">
            <column name="dataInicioTcc" length="45" />        
        </property>
        <property name="dataFimTcc" type="string">
            <column name="dataFimTcc" length="45" />        
        </property>
    
        <property name="dataInicioBanca" type="string">
            <column name="dataInicioBanca" length="45" />        
        </property>
        <property name="dataFimBanca" type="string">
            <column name="dataFimBanca" length="45" />        
        </property>
        <property name="dataInicioTccFinal" type="string">
            <column name="dataInicioTccFinal" length="45" />        
        </property>
        <property name="dataFinalTccFinal" type="string">
            <column name="dataFinalTccFinal" length="45" />        
        </property>
    </class>
</hibernate-mapping>
